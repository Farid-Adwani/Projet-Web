{% for valeur in statistiques %}
    <div class="col-lg-3 col-md-6">
        <div class="count-box">
            <i class="bi {{ valeur.getIcon() }}"></i>
            <span data-purecounter-start="0" data-purecounter-end={{ valeur.getValue() }} data-purecounter-duration="1"
                  class="purecounter"></span>
            <p>{{ valeur.getName() }}</p>
        </div>
    </div>
{% endfor %}