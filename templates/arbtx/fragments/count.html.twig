{% for valeur in Counts %}
    <div class="col-lg-3 col-md-6">
        <div class="count-box">
            <i class="bi {{ valeur['class'] }}"></i>
            <span data-purecounter-start="0" data-purecounter-end={{ valeur['number'] }} data-purecounter-duration="1"
                  class="purecounter"></span>
            <p>{{ valeur['nom'] }}</p>
        </div>
    </div>
{% endfor %}